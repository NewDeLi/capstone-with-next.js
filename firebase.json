{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ], 
    "rewrites":[
      {
        "source": "**",
        "function": "nextServer"
      }
    ],
    "functions":{
      "source": ".",
      "runtime": "nodejs12"
    }
  }
}
